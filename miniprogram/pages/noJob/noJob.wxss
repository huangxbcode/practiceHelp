/* pages/start/start.wxss */
.container {
	width: 100%;
	height: 100vh;
	background-color:#EDEDED;
	display: flex;
	justify-content: center;
	align-items: center;
}
.noJob {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	align-items: center;
}
.noJob image {
	width: 300rpx;
	height: 300rpx;
}
.noJob text {
	display: block;
	text-align: center;
	color: #C1C1C1;
	margin-bottom: 10rpx;
}
.van-button {
	background-color: #3399ff !important;
	border: 1rpx solid #3399ff !important; 
}