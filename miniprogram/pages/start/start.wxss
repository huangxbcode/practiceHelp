/* pages/start/start.wxss */
.container {
	width: 100%;
	height: 100vh;
	background-color:#EDEDED;
	display: flex;
	justify-content: center;
	align-items: center;
}
.netMiss {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	align-items: center;
}
.netMiss image {
	width: 300rpx;
	height: 300rpx;
}
.netMiss text {
	display: block;
	text-align: center;
	color: #C1C1C1;
	margin-bottom: 10rpx;
}